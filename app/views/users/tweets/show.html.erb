<h2>Tweet</h2>
  <div class="card card-body border-primary">
    <%= @tweet.title %><br>
    <%= @tweet.description %>
      <div class="row justify-content-start px-3 small py-2">
        <p>
          Created At:<%= @tweet.created_at %>,Updated At:<%= @tweet.updated_at %>
        </p>
      </div>
  </div>

<div class="p-2">
  <%= link_to "Edit this tweet", edit_users_tweet_path(@tweet) %> |
  <%= link_to "Back to tweets", users_tweets_path %>
</div>
